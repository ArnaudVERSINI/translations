"04060182.xhp\n"
"hd_id3150372\n"
"37\n"
"help.text"
msgid "FDIST"
msgstr "FDIST"

#: 04060182.xhp
msgctxt ""
"04060182.xhp\n"
"par_id3152981\n"
"38\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_FVERT\">Calculates the values of an F distribution.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_FVERT\">Вычисляет значения F-распределения.</ahelp>"

