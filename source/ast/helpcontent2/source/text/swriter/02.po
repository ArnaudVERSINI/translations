"par_id3147410\n"
"13\n"
"help.text"
msgid "<ahelp hid=\"SW:METRICFIELD:DLG_PAGEPREVIEW_PRINTOPTIONS:MF_LMARGIN\" visibility=\"visible\">Specifies the left margin width.</ahelp>"
msgstr "<ahelp hid=\"SW:METRICFIELD:DLG_PAGEPREVIEW_PRINTOPTIONS:MF_LMARGIN\" visibility=\"visible\">Especifica l'anchor del marxe esquierdu.</ahelp>"

#: 10110000.xhp
