msgid "$type$ library '$library$'"
msgstr "$type$ bibliotēka '$library$'"

#. E4RNh
#: macromigration.src
msgctxt "STR_MIGRATING_LIBS"
msgid "migrating libraries ..."
msgstr "migrē bibliotēkas ..."

