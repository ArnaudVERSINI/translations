msgid "Insert Join"
msgstr "Įterpti jungtį"

#. isNxK
#: query.src
msgctxt "STR_QUERY_UNDO_REMOVECONNECTION"
msgid "Delete Join"
msgstr "Šalinti jungtį"

#. 9Uu5p
#: query.src
msgctxt "STR_QUERY_UNDO_SIZETABWIN"
msgid "Resize table window"
msgstr "Keisti lentelės lango dydį"

#. 3qaWu
#: query.src
