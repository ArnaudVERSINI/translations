msgctxt ""
"04060182.xhp\n"
"par_id3150534\n"
"29\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_FTEST\">Returns the result of an F test.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_FTEST\">Vrne rezultat F-preizkusa.</ahelp>"

