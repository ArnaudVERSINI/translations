#: 04060182.xhp
msgctxt ""
"04060182.xhp\n"
"hd_id3146900\n"
"115\n"
"help.text"
msgid "Syntax"
msgstr "Cú pháp"

#: 04060182.xhp
msgctxt ""
"04060182.xhp\n"
"par_id3149287\n"
"116\n"
"help.text"
msgid "HARMEAN(Number1; Number2; ...Number30)"
msgstr "HARMEAN(Số1; Số2; ...; Số30)"

