"18010000.xhp\n"
"par_id3148520\n"
"2\n"
"help.text"
msgid "<ahelp hid=\".uno:SelectObject\">Allows you to select objects in the current document.</ahelp>"
msgstr "<ahelp hid=\".uno:SelectObject\">អនុញ្ញាត​ឲ្យ​អ្នក​ជ្រើស​វត្ថុ​នៅ​ក្នុង​ឯកសារ​បច្ចុប្បន្ន ។</ahelp>"

