"05120000.xhp\n"
"bm_id3153189\n"
"help.text"
msgid "<bookmark_value>conditional formatting; conditions</bookmark_value>"
msgstr "<bookmark_value>tingimuslik vormindamine; tingimused</bookmark_value>"

