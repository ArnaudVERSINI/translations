msgstr "<bookmark_value>JUROSACUM (função)</bookmark_value>"

#: 04060103.xhp
msgctxt ""
"04060103.xhp\n"
"hd_id3145257\n"
"335\n"
"help.text"
msgid "ACCRINT"
