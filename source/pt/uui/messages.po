msgstr "Memori~zar palavra-passe até ao fim da sessão"

#: strings.hrc:42
msgctxt "RID_SAVE_PASSWORD"
msgid "~Remember password"
msgstr "Memori~zar palavra-passe"

#: strings.hrc:43
msgctxt "STR_WARNING_INCOMPLETE_ENCRYPTION_TITLE"
