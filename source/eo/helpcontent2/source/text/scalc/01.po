#: 04090000.xhp
msgctxt ""
"04090000.xhp\n"
"par_id3153192\n"
"2\n"
"help.text"
msgid "<ahelp hid=\"SC_PUSHBUTTON_RID_SCDLG_LINKAREA_BTN_BROWSE\" visibility=\"hidden\">Locate the file containing the data you want to insert.</ahelp>"
msgstr "<ahelp hid=\"SC_PUSHBUTTON_RID_SCDLG_LINKAREA_BTN_BROWSE\" visibility=\"hidden\">Serĉi la dosieron kiu enhavas la datumojn enmetotajn.</ahelp>"

#: 04090000.xhp
