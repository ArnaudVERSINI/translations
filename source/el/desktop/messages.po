#. buqgv
#: dp_gui_dialog.src
msgctxt "RID_STR_ERROR_UNKNOWN_STATUS"
msgid "Error: The status of this extension is unknown"
msgstr "Σφάλμα: Η κατάσταση αυτής της επέκτασης είναι άγνωστη"

