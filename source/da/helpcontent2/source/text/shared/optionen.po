"par_id3154347\n"
"14\n"
"help.text"
msgid "<ahelp hid=\"SW_CHECKBOX_TP_CONTENT_OPT_CB_HSCROLL\">Displays the horizontal scrollbar.</ahelp>"
msgstr "<ahelp hid=\"SW_CHECKBOX_TP_CONTENT_OPT_CB_HSCROLL\">Viser den vandrette rullebjælke.</ahelp>"

#: 01040200.xhp
msgctxt ""
"01040200.xhp\n"
"hd_id3148663\n"
"15\n"
"help.text"
msgid "Vertical scrollbar"
msgstr "Lodret rullebjælke"

