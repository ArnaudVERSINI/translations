#: macromigration.src
msgctxt "STR_STATE_CLOSE_SUB_DOCS"
msgid "Prepare"
msgstr "Προετοιμασία"

#. pFNR3
#: macromigration.src
msgctxt "STR_STATE_BACKUP_DBDOC"
msgid "Backup Document"
msgstr "Εφεδρικό έγγραφο"

#. LDfr3
#: macromigration.src
msgctxt "STR_STATE_MIGRATE"
msgid "Migrate"
msgstr "Μετακόμιση"

#. tEttZ
#: macromigration.src
msgctxt "STR_STATE_SUMMARY"
msgid "Summary"
msgstr "Περίληψη"

