"help.text"
msgid "Street"
msgstr "Utca"

#: 01010100.xhp
msgctxt ""
"01010100.xhp\n"
"par_id3151212\n"
"10\n"
"help.text"
msgid "<ahelp hid=\"SVX:EDIT:RID_SFXPAGE_GENERAL:ED_STREET\">Type the name of your street in this field.</ahelp>"
msgstr "<ahelp hid=\"SVX:EDIT:RID_SFXPAGE_GENERAL:ED_STREET\">Ebbe a mezőbe írja be utcája nevét.</ahelp>"

#: 01010100.xhp
msgctxt ""
"01010100.xhp\n"
"hd_id3154909\n"
"11\n"
"help.text"
msgid "ZIP"
msgstr "Irányítószám"

#: 01010100.xhp
msgctxt ""
"01010100.xhp\n"
"par_id3145607\n"
"12\n"
"help.text"
msgid "<ahelp hid=\"SVX:EDIT:RID_SFXPAGE_GENERAL:ED_PLZ\">Type your ZIP in this field.</ahelp>"
msgstr "<ahelp hid=\"SVX:EDIT:RID_SFXPAGE_GENERAL:ED_PLZ\">Ebbe a mezőbe írja be irányítószámát.</ahelp>"

#: 01010100.xhp
msgctxt ""
"01010100.xhp\n"
"hd_id3154685\n"
"35\n"
"help.text"
msgid "City"
