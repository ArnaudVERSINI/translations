"par_id3155176\n"
"20\n"
"help.text"
msgid "<ahelp hid=\"SVX_CHECKBOX_RID_SFXPAGE_SAVE_BTN_RELATIVE_FSYS\">Select this box for <link href=\"text/shared/00/00000005.xhp#speichern\" name=\"relative saving\">relative saving</link> of URLs in the file system.</ahelp>"
msgstr "<ahelp hid=\"SVX_CHECKBOX_RID_SFXPAGE_SAVE_BTN_RELATIVE_FSYS\">Hautatu koadro hau fitxategi-sistemako URLak<link href=\"text/shared/00/00000005.xhp#speichern\" name=\"relative saving\"> erlatiboki gordetzeko</link>.</ahelp>"

