"01060300.xhp\n"
"par_id3150084\n"
"23\n"
"help.text"
msgid "<ahelp hid=\"SC:METRICFIELD:RID_SCPAGE_LAYOUT:MF_TAB\">Defines the tab stops distance.</ahelp>"
msgstr "<ahelp hid=\"SC:METRICFIELD:RID_SCPAGE_LAYOUT:MF_TAB\">Määrab tabelduskohtade sammu.</ahelp>"

#: 01060300.xhp
