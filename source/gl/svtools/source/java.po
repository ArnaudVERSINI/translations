msgid "JRE Required"
msgstr "Requírese JRE"

#: javaerror.src
msgctxt ""
"javaerror.src\n"
"STR_WARNING_INVALIDJAVASETTINGS\n"
"string.text"
msgid "Select JRE"
msgstr "Seleccionar JRE"

#: javaerror.src
msgctxt ""
"javaerror.src\n"
"STR_QUESTION_JAVADISABLED\n"
"string.text"
msgid "Enable JRE"
msgstr "Activar JRE"

