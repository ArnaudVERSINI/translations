msgctxt ""
"04060115.xhp\n"
"par_id3145138\n"
"23\n"
"help.text"
msgid "<item type=\"input\">=BIN2DEC(1100100)</item> returns 100."
msgstr "=BIN2DEC(1100100)ले १०० फर्काउँदछ।"

