msgstr "kopija do $location$ składowana"

#. BrPwb
#: macromigration.src
msgctxt "STR_MOVED_LIBRARY"
msgid "migrated $type$ library '$old$' to '$new$'"
msgstr "biblioteka $type$ '$old$' do '$new$' migrowana"

#. C7CAR
#: macromigration.src
