"par_id3149734\n"
"10\n"
"help.text"
msgid "<ahelp hid=\"HID_ICCDIALOG_CANCEL_BTN\">Closes the dialog without saving.</ahelp>"
msgstr "<ahelp hid=\"HID_ICCDIALOG_CANCEL_BTN\">Cierra el diálogo sin guardar.</ahelp>"

