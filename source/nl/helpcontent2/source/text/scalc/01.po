"04060184.xhp\n"
"par_id3154298\n"
"103\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_POISSON\">Returns the Poisson distribution.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_POISSON\">Geeft als resultaat de Poisson verdeling.</ahelp>"

#: 04060184.xhp
