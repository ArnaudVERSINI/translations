msgctxt ""
"04060106.xhp\n"
"par_id3152028\n"
"439\n"
"help.text"
msgid "SUMIF(Range; Criteria; SumRange)"
msgstr "SUMIF(ཁྱབ་ཚད། ཁྱད་ཚད། ཁྱབ་ཚད་བསྡོམས།(_r))"

