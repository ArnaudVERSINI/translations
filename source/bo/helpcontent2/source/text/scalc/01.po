msgctxt ""
"04060116.xhp\n"
"par_id3143229\n"
"165\n"
"help.text"
msgid "<item type=\"input\">=COMPLEX(3;4;\"j\")</item> returns 3+4j."
msgstr "=COMPLEX(3;4;j) ཕྱིར་ལོག་ 3+4j"

