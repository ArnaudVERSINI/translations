msgctxt ""
"indenting.xhp\n"
"par_id1491134\n"
"help.text"
msgid "<link href=\"text/shared/01/05030100.xhp\">Format - Paragraph - Indents & Spacing</link>"
msgstr "<link href=\"text/shared/01/05030100.xhp\">Formátum - Bekezdés - Behúzás és térköz</link>"

