msgstr "COVAR(data 1; data 2)"

#: 04060183.xhp
msgctxt ""
"04060183.xhp\n"
"par_id3145827\n"
"43\n"
"help.text"
msgid "<emph>Data1</emph> is the first data set."
msgstr "<emph>data 1</emph> ནི་གཞི་གྲངས་ཚོགས་ཨང་དང་པོ་ཡིན།"

#: 04060183.xhp
msgctxt ""
"04060183.xhp\n"
"par_id3150465\n"
"44\n"
"help.text"
msgid "<emph>Data2</emph> is the second data set."
msgstr "<emph>data 2</emph> ནི་གཞི་གྲངས་ཚོགས་གཉིས་པ་ཡིན།"

#: 04060183.xhp
msgctxt ""
"04060183.xhp\n"
"hd_id3154677\n"
"45\n"
"help.text"
msgid "Example"
msgstr "དཔེ་གཞི།"

#: 04060183.xhp
msgctxt ""
