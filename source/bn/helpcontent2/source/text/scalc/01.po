msgctxt ""
"04070400.xhp\n"
"par_id3147005\n"
"10\n"
"help.text"
msgid "<ahelp hid=\"SC:PUSHBUTTON:RID_SCDLG_COLROWNAMERANGES:BTN_ADD\">Adds the current label range to the list.</ahelp>"
msgstr "<ahelp hid=\"SC:PUSHBUTTON:RID_SCDLG_COLROWNAMERANGES:BTN_ADD\">তালিকায় বর্তমান লেবেলের পরিসর যোগ করে।</ahelp>"

