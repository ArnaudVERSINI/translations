msgctxt ""
"01170101.xhp\n"
"par_id3148485\n"
"122\n"
"help.text"
msgid "<ahelp hid=\".\">Specify the upper limit for a date control.</ahelp>"
msgstr "<ahelp hid=\".\">Too`annaa guyyaaf daangaa gubbaa ifteessi.</ahelp>"

