msgctxt ""
"04060116.xhp\n"
"par_id7844477\n"
"help.text"
msgid "<item type=\"input\">=FACTDOUBLE(5)</item> returns 15."
msgstr "<item type=\"input\">=FACTDOUBLE(5)</item> vrátí 15."

