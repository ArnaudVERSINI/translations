"help.text"
msgid "<ahelp hid=\"SVX_CHECKBOX_RID_SFXPAGE_SAVE_BTN_RELATIVE_INET\">Select this box for <link href=\"text/shared/00/00000005.xhp#speichern\" name=\"relative saving\">relative saving</link> of URLs to the Internet.</ahelp>"
msgstr "<ahelp hid=\"SVX_CHECKBOX_RID_SFXPAGE_SAVE_BTN_RELATIVE_INET\">Hautatu koadro hau Interneteko URLak <link href=\"text/shared/00/00000005.xhp#speichern\" name=\"relative saving\">erlatiboki gordetzeko</link>.</ahelp>"

