msgctxt ""
"04060116.xhp\n"
"par_id3147501\n"
"61\n"
"help.text"
msgid "IMPOWER(\"ComplexNumber\"; Number)"
msgstr "COMPLEXE.PUISSANCE(\"nombre_complexe\";nombre)"

#: 04060116.xhp
msgctxt ""
"04060116.xhp\n"
"par_id3155743\n"
"63\n"
"help.text"
msgid "<emph>Number</emph> is the exponent."
msgstr "<emph>nombre</emph> est l'exposant."

#: 04060116.xhp
msgctxt ""
"04060116.xhp\n"
"hd_id3149048\n"
"64\n"
"help.text"
msgid "Example"
msgstr "Exemple"

