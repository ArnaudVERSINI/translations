msgctxt ""
"01170101.xhp\n"
"par_id1134067\n"
"help.text"
msgid "<ahelp hid=\".\">Specifies the selection mode that is enabled for this tree control.</ahelp>"
msgstr "<ahelp hid=\".\">Ghi rõ chế độ lựa chọn mà được hiệu lực cho điều khiển cây này.</ahelp>"

#: 01170101.xhp
msgctxt ""
"01170101.xhp\n"
"hd_id3154193\n"
