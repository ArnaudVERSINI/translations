"par_idN10573\n"
"help.text"
msgid "<ahelp hid=\".\">Select the type of database that you want to connect to.</ahelp>"
msgstr "<ahelp hid=\".\">Selecione o tipo de banco de dados a que deseja se conectar.</ahelp>"

