msgstr "LARGE(tiedot; sija_C)"

#: 04060183.xhp
msgctxt ""
"04060183.xhp\n"
"par_id3152986\n"
"6\n"
"help.text"
msgid "<emph>Data</emph> is the cell range of data."
msgstr "<emph>Tiedot</emph> on tietojen solualue."

