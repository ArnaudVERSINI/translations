msgctxt ""
"04060184.xhp\n"
"par_id3147340\n"
"5\n"
"help.text"
msgid "MAX(Number1; Number2; ...Number30)"
msgstr "MAX(luku1; luku2; ...luku30)"

#: 04060184.xhp
#, fuzzy
