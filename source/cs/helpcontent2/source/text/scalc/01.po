msgctxt ""
"04060116.xhp\n"
"par_id3154116\n"
"43\n"
"help.text"
msgid "<item type=\"input\">=FACTDOUBLE(6)</item> returns 48."
msgstr "<item type=\"input\">=FACTDOUBLE(6)</item> vrátí 48."

