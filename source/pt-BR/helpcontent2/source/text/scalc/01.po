"04060103.xhp\n"
"par_id3147427\n"
"362\n"
"help.text"
msgid "AMORDEGRC(Cost; DatePurchased; FirstPeriod; Salvage; Period; Rate; Basis)"
msgstr "AMORDEGRC(Custo; DataCompra; PrimeiroPeríodo; ValorResidual; Período; Taxa; Base)"

