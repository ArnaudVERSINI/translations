msgctxt ""
"01040200.xhp\n"
"par_id3149202\n"
"18\n"
"help.text"
msgid "<ahelp hid=\"SW_LISTBOX_TP_CONTENT_OPT_LB_HMETRIC\">Displays the horizontal ruler. Select the desired measurement unit from the corresponding list.</ahelp>"
msgstr "<ahelp hid=\"SW_LISTBOX_TP_CONTENT_OPT_LB_HMETRIC\">Exibe a régua horizontal. Selecione a unidade de medida desejada na lista correspondente.</ahelp>"

#: 01040200.xhp
msgctxt ""
"01040200.xhp\n"
"hd_id3153104\n"
"19\n"
"help.text"
msgid "Vertical ruler"
