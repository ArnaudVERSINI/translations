msgctxt ""
"01160200.xhp\n"
"par_idN105AA\n"
"help.text"
msgid "Delete"
msgstr "Sil"

#: 01160200.xhp
msgctxt ""
"01160200.xhp\n"
"par_idN105AE\n"
"help.text"
msgid "<ahelp hid=\"809226770\">Removes the selected entry from the list.</ahelp>"
msgstr "<ahelp hid=\"809226770\">Seçili girdiyi listeden siler.</ahelp>"

#: 01160200.xhp
msgctxt ""
"01160200.xhp\n"
"par_idN105B1\n"
"help.text"
msgid "Edit"
