msgctxt ""
"06990000.xhp\n"
"tit\n"
"help.text"
msgid "Cell Contents"
msgstr "Conteúdo de célula"

#: 06990000.xhp
msgctxt ""
"06990000.xhp\n"
"hd_id3153087\n"
