"hd_id3149988\n"
"1\n"
"help.text"
msgid "<link href=\"text/shared/02/12100000.xhp\" name=\"Sort Order\">Sort Order</link>"
msgstr "<link href=\"text/shared/02/12100000.xhp\" name=\"Sort Order\">Ordenar</link>"

