msgstr "%EXTENSION_NAME ସକ୍ରିୟ କରିବା ଦ୍ୱାରା"

#. Mh7ag
#: dp_gui_dialog.src
msgctxt "RID_STR_DISABLING_PACKAGES"
msgid "Disabling %EXTENSION_NAME"
msgstr "%EXTENSION_NAME ନିଷ୍କ୍ରିୟ କରିବା ଦ୍ୱାରା"

#. GjgyB
#: dp_gui_dialog.src
msgctxt "RID_STR_ACCEPT_LICENSE"
