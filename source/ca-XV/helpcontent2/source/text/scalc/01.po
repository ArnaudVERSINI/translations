#: 04060116.xhp
msgctxt ""
"04060116.xhp\n"
"hd_id3154959\n"
"44\n"
"help.text"
msgid "IMABS"
msgstr "ABSIM"

#: 04060116.xhp
msgctxt ""
"04060116.xhp\n"
"par_id3149895\n"
"45\n"
"help.text"
msgid "<ahelp hid=\"HID_AAI_FUNC_IMABS\">The result is the absolute value of a complex number.</ahelp>"
msgstr "<ahelp hid=\"HID_AAI_FUNC_IMABS\">El resultat és el valor absolut d'un nombre complex.</ahelp>"

#: 04060116.xhp
msgctxt ""
"04060116.xhp\n"
"hd_id3155382\n"
"46\n"
"help.text"
msgid "Syntax"
msgstr "Sintaxi"

#: 04060116.xhp
msgctxt ""
