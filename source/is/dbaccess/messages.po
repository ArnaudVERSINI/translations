#: macromigration.src
msgctxt "STR_STATE_CLOSE_SUB_DOCS"
msgid "Prepare"
msgstr "Undirbúa"

#. pFNR3
#: macromigration.src
msgctxt "STR_STATE_BACKUP_DBDOC"
msgid "Backup Document"
msgstr "Öryggisafrit af skjalinu"

