msgctxt ""
"12090102.xhp\n"
"par_id3145257\n"
"10\n"
"help.text"
msgid "<ahelp hid=\"SC:CHECKBOX:RID_SCDLG_PIVOT_LAYOUT:BTN_DETECTCAT\">Automatically assigns rows without labels to the category of the row above.</ahelp>"
msgstr "<ahelp hid=\"SC:CHECKBOX:RID_SCDLG_PIVOT_LAYOUT:BTN_DETECTCAT\">Tildeler automatisk rækker uden etiketter til kategorien for rækken ovenover.</ahelp>"

