#. buqgv
#: dp_gui_dialog.src
msgctxt "RID_STR_ERROR_UNKNOWN_STATUS"
msgid "Error: The status of this extension is unknown"
msgstr "പിശക്: ഈ എക്സ്റ്റെന്‍ഷന്റെ അവസ്ഥ അപരിചിതം"

#. H6NGb
#: dp_gui_dialog.src
msgctxt "RID_STR_CLOSE_BTN"
msgid "Close"
msgstr "അടയ്ക്കുക"

#. T9Gqg
#: dp_gui_dialog.src
msgctxt "RID_STR_EXIT_BTN"
