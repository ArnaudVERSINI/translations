msgctxt ""
"04060103.xhp\n"
"par_id3147246\n"
"401\n"
"help.text"
msgid "<item type=\"input\">=RECEIVED(\"1999-02-15\";\"1999-05-15\";1000;0.0575;2)</item> returns 1014.420266."
msgstr "<item type=\"input\">=CANTIDÁ.RECIBIDA (\"1999-02-15\";\"1999-05-15\";1000;0.0575;2)</item> devuelve 1.014,420266."

