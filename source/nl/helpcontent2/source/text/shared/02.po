msgctxt ""
"09070400.xhp\n"
"hd_id3151110\n"
"12\n"
"help.text"
msgid "File type"
msgstr "Bestandstype"

#: 09070400.xhp
msgctxt ""
"09070400.xhp\n"
"par_id3153681\n"
"13\n"
"help.text"
msgid "<ahelp hid=\"SVX:LISTBOX:RID_SVXPAGE_HYPERLINK_NEWDOCUMENT:LB_DOCUMENT_TYPES\">Specifies the file type for the new document.</ahelp>"
msgstr "<ahelp hid=\"SVX:LISTBOX:RID_SVXPAGE_HYPERLINK_NEWDOCUMENT:LB_DOCUMENT_TYPES\">Specificeert het bestandstype voor het document dat moet worden gemaakt.</ahelp>"

