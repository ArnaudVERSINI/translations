msgid "<item type=\"input\">=PHI(2.25) </item>= 0.03"
msgstr "PHI(2.25) = ०.०३"

#: 04060184.xhp
msgctxt ""
"04060184.xhp\n"
"par_id3143236\n"
"99\n"
"help.text"
