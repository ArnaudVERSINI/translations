"par_idN11B74\n"
"help.text"
msgid "<ahelp hid=\"HID_PROP_DEFAULT_SCROLLVALUE\">Sets the default value for the scrollbar.</ahelp>"
msgstr "<ahelp hid=\"HID_PROP_DEFAULT_SCROLLVALUE\">Define o valor predefinido da barra de desprazamento.</ahelp>"

#: 01170101.xhp
