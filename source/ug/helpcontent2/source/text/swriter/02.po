"par_id3149691\n"
"help.text"
msgid "<image id=\"img_id3155174\" src=\"cmd/sc_insertcolumns.png\" width=\"0.222inch\" height=\"0.222inch\"><alt id=\"alt_id3155174\">Icon</alt></image>"
msgstr "<image id=\"img_id3155174\" src=\"cmd/sc_insertcolumns.png\" width=\"0.222inch\" height=\"0.222inch\"><alt id=\"alt_id3155174\">سىنبەلگە</alt></image>"

