msgctxt ""
"03030201.xhp\n"
"par_id3163798\n"
"10\n"
"help.text"
msgid "Print Hour(TimeSerial(12,30,41))"
msgstr "Print Hour(TimeSerial(12,30,41))"

#: 03030201.xhp
msgctxt ""
"03030201.xhp\n"
"par_id3155132\n"
"11\n"
"help.text"
msgid "returns the value 12."
msgstr "возвращает значение 12."

