#. sdbEf
#: alreadyopen.src
msgctxt "STR_ALREADYOPEN_TITLE"
msgid "Document in Use"
msgstr "Skjal í notkun"

#. ckcJc
#: alreadyopen.src
msgctxt "STR_ALREADYOPEN_MSG"
msgid ""
"Document file '$(ARG1)' is locked for editing by yourself on a different system since $(ARG2)\n"
"\n"
"Open document read-only, or ignore own file locking and open the document for editing.\n"
"\n"
msgstr ""
"Skjal '$(ARG1)' er læst fyrir breytingum af þér á annarri tölvu síðan $(ARG2)\n"
"\n"
"Opnaðu skjalið í lesham, eða hunsaðu skráarlásinn og opnaðu skjalið fyrir breytingar.\n"
"\n"

#. 8mKMg
#: alreadyopen.src
msgctxt "STR_ALREADYOPEN_READONLY_BTN"
msgid "Open ~Read-Only"
msgstr "Opna ~skrifvarið"

#. ThAZk
#: alreadyopen.src
msgctxt "STR_ALREADYOPEN_OPEN_BTN"
msgid "~Open"
msgstr "~Opna"

#. omvEC
#: alreadyopen.src
msgctxt "STR_ALREADYOPEN_SAVE_MSG"
msgid ""
"Document file '$(ARG1)' is locked for editing by yourself on a different system since $(ARG2)\n"
"\n"
"Close document on other system and retry saving or ignore own file locking and save current document.\n"
"\n"
msgstr ""
"Skjal '$(ARG1)' er læst fyrir breytingum af þér á annarri tölvu síðan $(ARG2)\n"
"\n"
"Lokaðu skjalinu á hinni tölvunni og reyndu að vista aftur eða hunsaðu skráarlásinn og vistaðu skjalið.\n"
"\n"

#. ZCJGW
#: alreadyopen.src
msgctxt "STR_ALREADYOPEN_RETRY_SAVE_BTN"
msgid "~Retry Saving"
msgstr "~Reyna aftur vistun"

#. EVEQx
#: alreadyopen.src
msgctxt "STR_ALREADYOPEN_SAVE_BTN"
msgid "~Save"
msgstr "~Vista"

#. PFEwD
#: filechanged.src
msgctxt "STR_FILECHANGED_TITLE"
msgid "Document Has Been Changed by Others"
msgstr "Skjali hefur verið breytt af öðrum"

#. XGC5m
#: filechanged.src
msgctxt "STR_FILECHANGED_MSG"
msgid ""
"The file has been changed since it was opened for editing in %PRODUCTNAME. Saving your version of the document will overwrite changes made by others.\n"
"\n"
"Do you want to save anyway?\n"
"\n"
msgstr ""
"Skránni hefur verið breytt frá því að þú opnaðir skjalið í %PRODUCTNAME. Ef þú vistar skjalið muntu skrifa yfir breytingar sem hafa verið gerðar af öðrum.\n"
"\n"
"Viltu samt halda áfram og vista?\n"
"\n"

#. DGYmK
#: filechanged.src
msgctxt "STR_FILECHANGED_SAVEANYWAY_BTN"
msgid "~Save Anyway"
msgstr "~Vista samt"

#. SZb7E
#: ids.src
msgctxt "RID_KEEP_PASSWORD"
msgid "~Remember password until end of session"
msgstr "~Muna lykilorð til loka setu"

#. 7HtCZ
#: ids.src
msgctxt "RID_SAVE_PASSWORD"
msgid "~Remember password"
msgstr "~Muna lykilorð"

#. CV6Ci
