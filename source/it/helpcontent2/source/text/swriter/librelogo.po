msgctxt ""
"LibreLogo.xhp\n"
"par_2660\n"
"help.text"
msgid "PRINT FINDALL(“\\w+”, “Dogs, cats.”) ; print [“Dogs”, “cats”], the list of the words.<br/>"
msgstr "STAMPA TROVATUTTO(“\\w+”, “Dogs, cats.”) ; stampa [“Dogs”, “cats”], l'elenco delle parole.<br/>"

