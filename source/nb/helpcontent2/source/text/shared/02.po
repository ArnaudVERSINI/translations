msgid "<image src=\"cmd/sc_gotoendofdoc.png\" id=\"img_id3153394\"><alt id=\"alt_id3153394\">Icon</alt></image>"
msgstr "<image src=\"cmd/sc_gotoendofdoc.png\" id=\"img_id3153394\"><alt id=\"alt_id3153394\">Ikon</alt></image>"

