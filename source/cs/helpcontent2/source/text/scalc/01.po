"par_id3151172\n"
"98\n"
"help.text"
msgid "<item type=\"input\">=VLOOKUP(A1;D1:E100;2)</item>"
msgstr "<item type=\"input\">=VLOOKUP(A1;D1:E100;2)</item>"

#. Kscf
