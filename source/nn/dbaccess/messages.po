msgctxt "STR_QUERY_UNDO_TABWINSHOW"
msgid "Add Table Window"
msgstr "Legg til tabellvindauge"

#: strings.hrc:126
msgctxt "STR_QUERY_UNDO_MOVETABWIN"
msgid "Move table window"
msgstr "Flytt tabellvindauge"

#: strings.hrc:127
msgctxt "STR_QUERY_UNDO_INSERTCONNECTION"
msgid "Insert Join"
msgstr "Set inn samband"

#: strings.hrc:128
msgctxt "STR_QUERY_UNDO_REMOVECONNECTION"
msgid "Delete Join"
msgstr "Slett samanslåing"

#: strings.hrc:129
msgctxt "STR_QUERY_UNDO_SIZETABWIN"
msgid "Resize table window"
msgstr "Endra storleik på tabellvindauget"

#: strings.hrc:130
msgctxt "STR_QUERY_UNDO_TABFIELDDELETE"
msgid "Delete Column"
msgstr "Slett kolonne"

#: strings.hrc:131
msgctxt "STR_QUERY_UNDO_TABFIELDMOVED"
msgid "Move column"
msgstr "Flytt kolonne"

#: strings.hrc:132
msgctxt "STR_QUERY_UNDO_TABFIELDCREATE"
msgid "Add Column"
msgstr "Legg til kolonne"

#: strings.hrc:133
msgctxt "RID_STR_FIELD_DOESNT_EXIST"
msgid "Invalid expression, field name '$name$' does not exist."
msgstr "Ugyldig uttrykk. Feltnamnet «$name$» finst ikkje."

#: strings.hrc:134
msgctxt "STR_QUERY_UNDO_TABWINDELETE"
msgid "Delete Table Window"
msgstr "Slett tabellvindauge"

#: strings.hrc:135
msgctxt "STR_QUERY_UNDO_MODIFY_CELL"
msgid "Edit Column Description"
msgstr "Rediger kolonneskildring"

#: strings.hrc:136
msgctxt "STR_QUERY_UNDO_SIZE_COLUMN"
msgid "Adjust column width"
msgstr "Juster kolonnebreidd"

#: strings.hrc:137
msgctxt "STR_QUERY_SORTTEXT"
msgid "(not sorted);ascending;descending"
msgstr "(ikkje sortert);stigande;fallande"

#: strings.hrc:138
msgctxt "STR_QUERY_FUNCTIONS"
msgid "(no function);Group"
msgstr "(ingen funksjon);Gruppe"

#: strings.hrc:139
msgctxt "STR_QUERY_NOTABLE"
msgid "(no table)"
msgstr "(ingen tabell)"

#: strings.hrc:140
msgctxt "STR_QRY_ORDERBY_UNRELATED"
msgid "The database only supports sorting for visible fields."
msgstr "Denne databasen støttar berre sortering av synlege felt."

#: strings.hrc:141
msgctxt "STR_QUERY_HANDLETEXT"
msgid "Field;Alias;Table;Sort;Visible;Function;Criterion;Or;Or"
msgstr "Felt;Alias;Tabell;Sorter;Synleg;Funksjon;Kriterium;Eller;Eller"

#: strings.hrc:142
msgctxt "STR_QUERY_LIMIT_ALL"
msgid "All"
msgstr "Alle"

#: strings.hrc:143
msgctxt "STR_QRY_TOO_MANY_COLUMNS"
msgid "There are too many columns."
msgstr "Det er for mange kolonnar."

#: strings.hrc:144
msgctxt "STR_QRY_CRITERIA_ON_ASTERISK"
msgid "A condition cannot be applied to field [*]"
msgstr "Kan ikkje bruka vilkår på feltet [*]"

#: strings.hrc:145
msgctxt "STR_QRY_TOO_LONG_STATEMENT"
msgid "The SQL statement created is too long."
msgstr "SQL-setninga er for lang."

#: strings.hrc:146
msgctxt "STR_QRY_TOOCOMPLEX"
msgid "Query is too complex"
msgstr "Spørjinga er for kompleks"

#: strings.hrc:147
msgctxt "STR_QRY_NOSELECT"
msgid "Nothing has been selected."
msgstr "Ingenting er markert."

#: strings.hrc:148
msgctxt "STR_QRY_SYNTAX"
msgid "SQL syntax error"
msgstr "SQL-syntaksfeil"

#: strings.hrc:149
msgctxt "STR_QRY_ORDERBY_ON_ASTERISK"
msgid "[*] cannot be used as a sort criterion."
msgstr "Kan ikkje bruka [*] som søkjekriterium."

#: strings.hrc:150
msgctxt "STR_QRY_TOO_MANY_TABLES"
msgid "There are too many tables."
msgstr "Det er for mange tabellar."

#: strings.hrc:151
msgctxt "STR_QRY_NATIVE"
msgid "The statement will not be applied when querying in the SQL dialect of the database."
msgstr "Setninga kan ikkje brukast med SQL-dialekten til denne databasen."

#: strings.hrc:152
msgctxt "STR_QRY_ILLEGAL_JOIN"
msgid "Join could not be processed"
msgstr "Klarte ikkje handsama samanslåing"

#: strings.hrc:153
msgctxt "STR_SVT_SQL_SYNTAX_ERROR"
msgid "Syntax error in SQL statement"
msgstr "Syntaksfeil i SQL-setninga"

#: strings.hrc:154
msgctxt "STR_QUERYDESIGN_NO_VIEW_SUPPORT"
msgid "This database does not support table views."
msgstr "Denne databasen støttar ikkje tabellvising."

#: strings.hrc:155
msgctxt "STR_NO_ALTER_VIEW_SUPPORT"
msgid "This database does not support altering of existing table views."
msgstr "Denne databasen støttar ikkje endring av tabellvisingar."

#: strings.hrc:156
msgctxt "STR_QUERYDESIGN_NO_VIEW_ASK"
msgid "Do you want to create a query instead?"
msgstr "Vil du laga ei spørjing i staden?"

