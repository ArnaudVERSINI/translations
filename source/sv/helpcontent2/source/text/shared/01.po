msgstr "Representerar mellanslagstecken (men inte andra mellanrumstecken)."

#. ;R^1
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"par_id3150486\n"
"220\n"
"help.text"
msgid "[:print:]"
msgstr "[:print:]"

#. eVB_
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"par_id3150872\n"
"221\n"
"help.text"
msgid "Represents a printable character."
msgstr "Motsvarar ett utskrivbart tecken."

#. S_nB
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"par_id3155854\n"
"222\n"
"help.text"
msgid "[:cntrl:]"
msgstr "[:cntrl:]"

#. 6Q8/
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"par_id3152576\n"
"223\n"
"help.text"
msgid "Represents a nonprinting character."
msgstr "Motsvarar ett kontrolltecken."

#. R]s8
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"par_id3149958\n"
"228\n"
"help.text"
msgid "[:lower:]"
msgstr "[:lower:]"

#. O_t?
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"par_id3145730\n"
"229\n"
"help.text"
msgid "Represents a lowercase character if <emph>Match case</emph> is selected in <emph>Options</emph>."
msgstr "Motsvarar ett gement tecken om <emph>Exakt</emph> har markerats under <emph>Alternativ.</emph>"

#. e:u3
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"par_id3148455\n"
"230\n"
"help.text"
msgid "[:upper:]"
msgstr "[:upper:]"

#. 3+z,
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"par_id3150092\n"
"231\n"
"help.text"
msgid "Represents an uppercase character if <emph>Match case</emph> is selected in <emph>Options.</emph>"
msgstr "Motsvarar ett versalt tecken om <emph>Exakt</emph> har markerats under <emph>Alternativ.</emph>"

#. 0(j5
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"hd_id5311441\n"
"help.text"
msgid "Examples"
msgstr "Exempel"

#. (V;-
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"par_id956834773\n"
"help.text"
msgid "e([:digit:])?    -- finds 'e' followed by zero or one digit. Note that currently all named character classes like [:digit:] must be enclosed in parentheses."
msgstr "e([:digit:])?    -- hittar 'e' följt av ingen eller en siffra. Observera att alla namngivna klasser som exempelvis [:digit:] måste omslutas av en parentes."

#. U#)4
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"par_id952368773\n"
"help.text"
msgid "^([:digit:])$    -- finds lines or cells with exactly one digit."
msgstr "^([:digit:])$    -- hittar rader eller celler med exakt en siffra."

#. K=?j
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"par_id9568773\n"
"help.text"
msgid "You can combine the search terms to form complex searches."
msgstr "Du kan kombinera söktermer om du vill göra mer komplexa sökningar."

#. URnd
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"hd_id71413\n"
"help.text"
msgid "To find three-digit numbers alone in a paragraph"
msgstr "Så här hittar du tresiffriga tal i ett stycke"

#. x/^V
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"par_id2924283\n"
"help.text"
msgid "^[:digit:]{3}$"
msgstr "^[:digit:]{3}$"

#. `0xq
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"par_id5781731\n"
"help.text"
msgid "^ means the match has to be at the start of a paragraph,"
msgstr "^ innebär att matchen måste finnas i början av stycket,"

#. 9P9,
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"par_id6942045\n"
"help.text"
msgid "[:digit:] matches any decimal digit,"
msgstr "[:digit:] matchar valfritt decimaltecken,"

#. T)8a
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"par_id4721823\n"
"help.text"
msgid "{3} means there must be exactly 3 copies of \"digit\","
msgstr "{3} innebär att det måste finnas exakt tre kopior av \"tal\","

#. ~M:9
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"par_id5806756\n"
"help.text"
msgid "$ means the match must end a paragraph."
msgstr "$ innebär att matchen måste avsluta ett stycke."

