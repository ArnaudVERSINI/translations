#. KjDmE
#: conn_error_message.src
msgctxt "256 + 2*100 + 0"
msgid "The record operation has been vetoed."
msgstr "Opptaksoperasjonen er forhindret."

#. xAZXk
#: conn_error_message.src
msgctxt "256 + 2*200 + 0"
msgid "The statement contains a cyclic reference to one or more sub queries."
msgstr ""
"Uttrykket inneholder en syklisk referanse til en eller flere delspørringer."

#. sppzX
#: conn_error_message.src
msgctxt "256 + 2*300 + 0"
msgid "The name must not contain any slashes ('/')."
msgstr "Navnet kan ikke inneholde skråstreker («/»)."

#. pcGch
#: conn_error_message.src
msgctxt "256 + 2*301 + 0"
msgid "$1$ is no SQL conform identifier."
msgstr "$1$ er ikke en SQL-identifikator."

#. rhNKU
#: conn_error_message.src
msgctxt "256 + 2*302 + 0"
msgid "Query names must not contain quote characters."
msgstr "Spørringer kan ikke ha hermetegn i navnet."

#. vt9Xy
#: conn_error_message.src
msgctxt "256 + 2*303 + 0"
msgid "The name '$1$' is already in use in the database."
msgstr "Navnet «$1$» er brukt fra før i databasen."

#. mHdB5
#: conn_error_message.src
msgctxt "256 + 2*304 + 0"
msgid "No connection to the database exists."
msgstr "Har ingen tilkobling til databasen."

#. qsN7W
#: conn_error_message.src
msgctxt "256 + 2*500 + 0"
msgid "No $1$ exists."
msgstr "Har ingen $1$."

#. xvK2D
#: conn_error_message.src
msgctxt "256 + 2*550 + 0"
msgid "Unable to display the complete table content. Please apply a filter."
msgstr "Klarer ikke å vise hele tabellinnholdet. Du bør bruke et filter."

#. 9LXDp
#: conn_shared_res.src
msgctxt "STR_ERR_EXECUTING_QUERY"
msgid "An error occurred while executing the query."
msgstr "En feil oppstod ved kjøring av spørringen."

#. 7yRgf
#: conn_shared_res.src
msgctxt "STR_QUERY_AT_LEAST_ONE_TABLES"
msgid "The query can not be executed. It needs at least one table."
msgstr "Kan ikke utføre spørringa, det trengs minst en tabell."

#. aBhA8
#: conn_shared_res.src
msgctxt "STR_NO_COUNT_SUPPORT"
msgid "The driver does not support the 'COUNT' function."
msgstr "Driveren støtter ikke funksjonen «COUNT»."

#. FB59h
#: conn_shared_res.src
msgctxt "STR_STMT_TYPE_NOT_SUPPORTED"
msgid "This statement type not supported by this database driver."
msgstr "Denne databasedriveren støtter ikke denne typen uttrykk."

#. KodDj
#: conn_shared_res.src
msgctxt "STR_UNSPECIFIED_ERROR"
msgid "An unknown error occurred."
msgstr "Det oppstod en ukjent feil."

#. 2CZNZ
#: conn_shared_res.src
msgctxt "STR_ERROR_REFRESH_ROW"
msgid "An error occurred while refreshing the current row."
msgstr "Det oppsto en feil da gjeldende rad skulle oppdateres."

#. DJhQp
#: conn_shared_res.src
msgctxt "STR_ERROR_GET_ROW"
msgid "An error occurred while getting the current row."
msgstr "Det oppsto en feil da gjeldende rad skulle hentes inn."

#. s9ves
#: conn_shared_res.src
msgctxt "STR_QUERY_INVALID_IS_NULL_COLUMN"
msgid ""
"The query can not be executed. The 'IS NULL' can only be used with a column "
"name."
msgstr ""
"Kan ikke utføre spørringa; «IS NULL» kan bare brukes sammen med et "
"kolonnenavn."

#. kLhAy
#: conn_shared_res.src
msgctxt "STR_ILLEGAL_MOVEMENT"
msgid "Illegal cursor movement occurred."
msgstr "Skrivemerket ble flyttet på en ikke tillatt måte."

#. vGtCJ
#: conn_shared_res.src
msgctxt "STR_COMMIT_ROW"
msgid "Please commit row '$position$' before update rows or insert new rows."
msgstr ""
"Du må melde inn raden «$position$» før du kan oppdatere rader eller sette "
"inn nye."

#. 7DcRU
#: conn_shared_res.src
msgctxt "STR_NO_CONNECTION_GIVEN"
msgid "It doesn't exist a connection to the database."
msgstr "Ingen tilkobling til databasen."

#. 5BYEX
#: conn_shared_res.src
msgctxt "STR_WRONG_PARAM_INDEX"
msgid ""
"You tried to set a parameter at position '$pos$' but there is/are only "
"'$count$' parameter(s) allowed. One reason may be that the property "
"\"ParameterNameSubstitution\" is not set to TRUE in the data source."
msgstr ""
"Du prøvde å angi et parameter ved plasseringa «$pos$» men det er bare mulig "
"å bruke «$count$» antall parametre. En av grunnene kan være at egenskapen "
"«ParameterNameSubstitution» ikke er satt til «TRUE» i datakilden."

#. 6FnrV
#: conn_shared_res.src
msgctxt "STR_NO_INPUTSTREAM"
msgid "The input stream was not set."
msgstr "Innstrømmen (InputStream) ble ikke valgt"

#. Davdp
#: conn_shared_res.src
msgctxt "STR_NO_ELEMENT_NAME"
msgid "There is no element named '$name$'."
msgstr "Det finnes intet element ved navnet «$name$»"

#. CWktu
#: conn_shared_res.src
msgctxt "STR_INVALID_BOOKMARK"
msgid "Invalid bookmark value"
msgstr "Ugyldig bokmerkeverdi"

#. VXSEP
#: conn_shared_res.src
msgctxt "STR_PRIVILEGE_NOT_GRANTED"
msgid "Privilege not granted: Only table privileges can be granted."
msgstr "Rettigheten er ikke gitt; bare tabellrettigheter kan gis."

#. DZf3v
#: conn_shared_res.src
msgctxt "STR_PRIVILEGE_NOT_REVOKED"
msgid "Privilege not revoked: Only table privileges can be revoked."
msgstr ""
"Rettigheten er ikke trukket tilbake; bare tabellrettigheter kan trekkes "
"tilbake."

#. qTZj7
#: conn_shared_res.src
msgctxt "STR_ERRORMSG_SEQUENCE"
msgid "Function sequence error."
msgstr "Feil funksjonsrekkefølge."

#. scUDb
#: conn_shared_res.src
msgctxt "STR_INVALID_INDEX"
msgid "Invalid descriptor index."
msgstr "Ugyldig beskrivelsesindeks."

#. MAAeW
#: conn_shared_res.src
msgctxt "STR_UNSUPPORTED_FUNCTION"
msgid "The driver does not support the function '$functionname$'."
msgstr "Driveren støtter ikke funksjonen «$functionname$»."

#. FAp7x
#: conn_shared_res.src
msgctxt "STR_UNSUPPORTED_FEATURE"
msgid ""
"The driver does not support the functionality for '$featurename$'. It is not"
" implemented."
msgstr ""
"Driveren støtter ikke funksjonen «$featurename$», den er ikke implementert."

#. zXVCV
#: conn_shared_res.src
msgctxt "STR_FORMULA_WRONG"
msgid "The formula for TypeInfoSettings is wrong!"
msgstr "Formelen for «TypeInfoSettings» er feil."

#. ZWq6D
#: conn_shared_res.src
msgctxt "STR_STRING_LENGTH_EXCEEDED"
msgid ""
"The string '$string$' exceeds the maximum length of $maxlen$ characters when"
" converted to the target character set '$charset$'."
msgstr ""
"Strengen «$string$» er lengre enn grensa på $maxlen$ tegn når den blir gjort"
" om til tegnsettet «$charset$»."

#. CYSBr
#: conn_shared_res.src
msgctxt "STR_CANNOT_CONVERT_STRING"
msgid ""
"The string '$string$' cannot be converted using the encoding '$charset$'."
msgstr "Strengen '$string$' kan ikke gjøres om til tegnkodinga '$charset$'."

#. sSzsJ
#: conn_shared_res.src
msgctxt "STR_URI_SYNTAX_ERROR"
msgid "The connection URL is invalid."
msgstr "Tilkoblingsadressen er ugyldig."

#. ULTqE
#: conn_shared_res.src
msgctxt "STR_QUERY_TOO_COMPLEX"
msgid "The query can not be executed. It is too complex."
msgstr "Kan ikke utføre spørringa, den er for avansert."

#. UQYpN
#: conn_shared_res.src
msgctxt "STR_OPERATOR_TOO_COMPLEX"
msgid "The query can not be executed. The operator is too complex."
msgstr "Kan ikke utføre spørringa, operatoren er for avansert."

#. DmQcr
#: conn_shared_res.src
msgctxt "STR_QUERY_INVALID_LIKE_COLUMN"
msgid ""
"The query can not be executed. You cannot use 'LIKE' with columns of this "
"type."
msgstr ""
"Kan ikke utføre spørringa. Du kan ikke bruke «LIKE» med slike kolonner."

#. EMgKF
#: conn_shared_res.src
msgctxt "STR_QUERY_INVALID_LIKE_STRING"
msgid ""
"The query can not be executed. 'LIKE' can be used with a string argument "
"only."
msgstr ""
"Kan ikke utføre spørringa; «LIKE» kan bare brukes i kombinasjon med et "
"strengvalg."

#. PBG3H
#: conn_shared_res.src
msgctxt "STR_QUERY_NOT_LIKE_TOO_COMPLEX"
msgid ""
"The query can not be executed. The 'NOT LIKE' condition is too complex."
msgstr "Kan ikke utføre spørringa. Vilkåret «NOT LIKE» er for avansert."

#. CWeME
#: conn_shared_res.src
msgctxt "STR_QUERY_LIKE_WILDCARD"
msgid ""
"The query can not be executed. The 'LIKE' condition contains wildcard in the"
" middle."
msgstr "Kan ikke utføre spørringa fordi vilkåret «LIKE» har jokertegn midt i."

#. NK7eq
#: conn_shared_res.src
msgctxt "STR_QUERY_LIKE_WILDCARD_MANY"
msgid ""
"The query can not be executed. The 'LIKE' condition contains too many "
"wildcards."
msgstr ""
"Kan ikke utføre spørringa fordi vilkåret «LIKE» har for mange jokertegn."

#. nADdF
#: conn_shared_res.src
msgctxt "STR_INVALID_COLUMNNAME"
msgid "The column name '$columnname$' is not valid."
msgstr "Kolonnenavnet «$columnname$» er ugyldig."

#. FT3Zb
#: conn_shared_res.src
msgctxt "STR_INVALID_COLUMN_SELECTION"
msgid "The statement contains an invalid selection of columns."
msgstr "Uttrykket inneholder et ugyldig valg av kolonner."

#. sEFWB
#: conn_shared_res.src
msgctxt "STR_COLUMN_NOT_UPDATEABLE"
msgid "The column at position '$position$' could not be updated."
msgstr "Kolonnen på plasseringa «$position$» kunne ikke oppdateres."

#. iLNAb
#: conn_shared_res.src
msgctxt "STR_COULD_NOT_LOAD_FILE"
msgid "The file $filename$ could not be loaded."
msgstr "Klarte ikke å lese inn fila $filename$."

#. jq62z
#: conn_shared_res.src
msgctxt "STR_LOAD_FILE_ERROR_MESSAGE"
msgid ""
"The attempt to load the file resulted in the following error message ($exception_type$):\n"
"\n"
"$error_message$"
msgstr ""
"Forsøket på å lese inn fila ga følgende feilmelding ($exception_type$):\n"
"\n"
"$error_message$"

#. sbrdS
#: conn_shared_res.src
msgctxt "STR_TYPE_NOT_CONVERT"
msgid "The type could not be converted."
msgstr "Klarte ikke å endre typen."

#. 3L6uG
#: conn_shared_res.src
msgctxt "STR_INVALID_COLUMN_DESCRIPTOR_ERROR"
msgid "Could not append column: invalid column descriptor."
msgstr "Klarte ikke å legge til kolonnen: Ugyldig kolonnebeskrivelse."

#. 4GMmY
#: conn_shared_res.src
msgctxt "STR_INVALID_GROUP_DESCRIPTOR_ERROR"
msgid "Could not create group: invalid object descriptor."
msgstr "Klarte ikke å opprette gruppen: Ugyldig gruppebeskrivelse."

#. MDKgr
#: conn_shared_res.src
msgctxt "STR_INVALID_INDEX_DESCRIPTOR_ERROR"
msgid "Could not create index: invalid object descriptor."
msgstr "Klarte ikke å opprette indekset: Ugyldig objektbeskrivelse."

#. jPjxi
#: conn_shared_res.src
msgctxt "STR_INVALID_KEY_DESCRIPTOR_ERROR"
msgid "Could not create key: invalid object descriptor."
msgstr "Klarte ikke å opprette nøkkelen: Ugyldig objektbeskrivelse."

#. jaDH3
#: conn_shared_res.src
msgctxt "STR_INVALID_TABLE_DESCRIPTOR_ERROR"
msgid "Could not create table: invalid object descriptor."
msgstr "Klarte ikke å opprette tabellen: Ugyldig objektbeskrivelse."

#. utNzu
#: conn_shared_res.src
msgctxt "STR_INVALID_USER_DESCRIPTOR_ERROR"
msgid "Could not create user: invalid object descriptor."
msgstr "Klarte ikke å opprette brukeren: Ugyldig objektbeskrivelse."

#. 4TE9R
#: conn_shared_res.src
msgctxt "STR_INVALID_VIEW_DESCRIPTOR_ERROR"
msgid "Could not create view: invalid object descriptor."
msgstr "Klarte ikke å opprette visningen: Ugyldig objektbeskrivelse."

#. BrHQp
#: conn_shared_res.src
msgctxt "STR_VIEW_NO_COMMAND_ERROR"
msgid "Could not create view: no command object."
msgstr "Klarte ikke å opprette visningen: Kommandoobjekt mangler."

#. GgFCn
#: conn_shared_res.src
msgctxt "STR_NO_CONNECTION"
msgid ""
"The connection could not be created. May be the necessary data provider is "
"not installed."
msgstr ""
"Klarte ikke å opprette forbindelsen. Kanskje den ønskede dataleverandøren "
"ikke er installert."

#. GRZEu
#: conn_shared_res.src
msgctxt "STR_COULD_NOT_DELETE_INDEX"
msgid ""
"The index could not be deleted. An unknown error while accessing the file "
"system occurred."
msgstr ""
"Klarte ikke å slette registret. Det oppsto en ukjent feil da filsystemet "
"skulle brukes."

#. JbDnu
#: conn_shared_res.src
msgctxt "STR_ONL_ONE_COLUMN_PER_INDEX"
msgid "The index could not be created. Only one column per index is allowed."
msgstr ""
"Klarte ikke å opprette indekset. Det er bare tillatt med en kolonne per "
"indeks."

#. rB3XE
#: conn_shared_res.src
msgctxt "STR_COULD_NOT_CREATE_INDEX_NOT_UNIQUE"
msgid "The index could not be created. The values are not unique."
msgstr "Klarte ikke å opprette indekset. Verdiene er ikke unike."

#. f8DTu
#: conn_shared_res.src
msgctxt "STR_COULD_NOT_CREATE_INDEX"
msgid "The index could not be created. An unknown error appeared."
msgstr "Klarte ikke å opprette indekset. Det oppsto en ukjent feil."

#. HPZuZ
#: conn_shared_res.src
msgctxt "STR_COULD_NOT_CREATE_INDEX_NAME"
msgid ""
"The index could not be created. The file '$filename$' is used by an other "
"index."
msgstr ""
"Klarte ikke å opprette indekset. Fila «$filename$» brukes av et annet "
"indeks."

#. GcK7B
#: conn_shared_res.src
msgctxt "STR_COULD_NOT_CREATE_INDEX_KEYSIZE"
msgid ""
"The index could not be created. The size of the chosen column is too big."
msgstr ""
"Kunne ikke opprette registeret. Størrelsen på den valgte kolonna er for "
"stor."

#. vWZ84
#: conn_shared_res.src
msgctxt "STR_SQL_NAME_ERROR"
msgid "The name '$name$' doesn't match SQL naming constraints."
msgstr "Navnet «$name$» følger ikke SQLs navnebegrensninger."

#. wv2Cx
#: conn_shared_res.src
msgctxt "STR_COULD_NOT_DELETE_FILE"
msgid "The file $filename$ could not be deleted."
msgstr "Klarte ikke å slette fila $filename$."

#. rp3rF
#: conn_shared_res.src
msgctxt "STR_INVALID_COLUMN_TYPE"
msgid "Invalid column type for column '$columnname$'."
msgstr "Ugyldig kolonnetype for kolonnen «$columnname$»."

#. jAStU
#: conn_shared_res.src
msgctxt "STR_INVALID_COLUMN_PRECISION"
msgid "Invalid precision for column '$columnname$'."
msgstr "Ugyldig presisering for kolonnen «$columnname$»."

#. zJbtr
#: conn_shared_res.src
msgctxt "STR_INVALID_PRECISION_SCALE"
msgid "Precision is less than scale for column '$columnname$'."
msgstr "Størrelsen er mindre enn kolonnestørrelsen på «$columnname$»."

#. PDCV3
#: conn_shared_res.src
msgctxt "STR_INVALID_COLUMN_NAME_LENGTH"
msgid "Invalid column name length for column '$columnname$'."
msgstr "Ugyldig navnelengde på kolonnen «$columnname$»."

#. NZWGq
#: conn_shared_res.src
msgctxt "STR_DUPLICATE_VALUE_IN_COLUMN"
msgid "Duplicate value found in column '$columnname$'."
msgstr "Fant en dobbeltoppført verdi i kolonnen «$columnname$»."

#. sfaxE
#: conn_shared_res.src
msgctxt "STR_INVALID_COLUMN_DECIMAL_VALUE"
msgid ""
"The '$columnname$' column has been defined as a \"Decimal\" type, the max. length is $precision$ characters (with $scale$ decimal places).\n"
"\n"
"The specified value \"$value$ is longer than the number of digits allowed."
msgstr ""
"Kolonnen «$columnname$» er blitt angitt å være av typen «desimal», høyeste tillatte lengde er $precision$ tegn (med $scale$ antall desimaler).\n"
"\n"
"Den angitte lengden $value$ overstiger den øvre grensen."

#. ZvEz9
#: conn_shared_res.src
msgctxt "STR_COLUMN_NOT_ALTERABLE"
msgid ""
"The column '$columnname$' could not be altered. May be the file system is "
"write protected."
msgstr ""
"Kolonnen «$columnname$» kunne ikke endres. Kanskje filsystemet er "
"skrivebeskyttet?"

#. 4BgE9
#: conn_shared_res.src
msgctxt "STR_INVALID_COLUMN_VALUE"
msgid ""
"The column '$columnname$' could not be updated. The value is invalid for "
"that column."
msgstr ""
"Klarte ikke å oppdatere kolonnen «$columnname$». Verdien er ugyldig for den "
"kolonnen."

#. dFAFB
#: conn_shared_res.src
msgctxt "STR_COLUMN_NOT_ADDABLE"
msgid ""
"The column '$columnname$' could not be added. May be the file system is "
"write protected."
msgstr ""
"Kolonnen «$columnname$» kunne ikke legges til. Kanskje filsystemet er "
"skrivebeskyttet?"

#. zk3QB
#: conn_shared_res.src
msgctxt "STR_COLUMN_NOT_DROP"
msgid ""
"The column at position '$position$' could not be dropped. May be the file "
"system is write protected."
msgstr ""
"Kolonnen på plasseringa «$position$» kunne ikke utelates. Kanskje "
"filsystemet er skrivebeskyttet?"

#. hAwmi
#: conn_shared_res.src
msgctxt "STR_TABLE_NOT_DROP"
msgid ""
"The table '$tablename$' could not be dropped. May be the file system is "
"write protected."
msgstr ""
"Tabellen «$tablename$» kunne ikke utelates. Kanskje filsystemet er "
"skrivebeskyttet?"

#. R3BGx
#: conn_shared_res.src
msgctxt "STR_COULD_NOT_ALTER_TABLE"
msgid "The table could not be altered."
msgstr "Klarte ikke å endre tabellen."

#. UuoNm
#: conn_shared_res.src
msgctxt "STR_INVALID_DBASE_FILE"
msgid "The file '$filename$' is an invalid (or unrecognized) dBase file."
msgstr "Fila «$filename$» er en ugyldig (eller ikke gjenkjent) dBase-fil."

#. LhHTA
#: conn_shared_res.src
msgctxt "STR_CANNOT_OPEN_BOOK"
msgid "Cannot open Evolution address book."
msgstr "Klarer ikke å åpne adresseboka til Evolution."

#. sxbEF
#: conn_shared_res.src
msgctxt "STR_SORT_BY_COL_ONLY"
msgid "Can only sort by table columns."
msgstr "Klarer bare å sortere etter tabellkolonner."

#. E4wn2
#: conn_shared_res.src
msgctxt "STR_QUERY_COMPLEX_COUNT"
msgid ""
"The query can not be executed. It is too complex. Only \"COUNT(*)\" is "
"supported."
msgstr ""
"Kan ikke utføre spørringa, den er for avansert. Bare «COUNT(*)» er støttet."

#. 8VQo4
#: conn_shared_res.src
msgctxt "STR_QUERY_INVALID_BETWEEN"
msgid ""
"The query can not be executed. The 'BETWEEN' arguments are not correct."
msgstr "Kan ikke utføre spørringa, «BETWEEN»-argumentet er feil."

#. 4oK7N
#: conn_shared_res.src
msgctxt "STR_QUERY_FUNCTION_NOT_SUPPORTED"
msgid "The query can not be executed. The function is not supported."
msgstr "Kan ikke utføre spørringa, funksjonen støttes ikke."

#. kCjVU
#: conn_shared_res.src
msgctxt "STR_TABLE_READONLY"
msgid "The table can not be changed. It is read only."
msgstr "Tabellen kan ikke endres, da den er skrivebeskyttet."

#. cqWEv
#: conn_shared_res.src
msgctxt "STR_DELETE_ROW"
msgid ""
"The row could not be deleted. The option \"Display inactive records\" is "
"set."
msgstr "Kunne ikke slette raden fordi «Display inactive records» er valgt."

#. TZTfv
#: conn_shared_res.src
msgctxt "STR_ROW_ALREADY_DELETED"
msgid "The row could not be deleted. It is already deleted."
msgstr "Kunne ikke slette raden, den er slettet allerede."

#. fuJot
#: conn_shared_res.src
msgctxt "STR_QUERY_MORE_TABLES"
msgid "The query can not be executed. It contains more than one table."
msgstr "Kunne ikke utføre spørringa da den inneholder mer enn en tabell."

#. w7AzE
#: conn_shared_res.src
msgctxt "STR_QUERY_NO_TABLE"
msgid "The query can not be executed. It contains no valid table."
msgstr "Kunne ikke utføre spørringa da den ikke inneholder en gyldig tabell."

#. CRsGn
#: conn_shared_res.src
msgctxt "STR_QUERY_NO_COLUMN"
msgid "The query can not be executed. It contains no valid columns."
msgstr "Kunne ikke utføre spørringa da den ikke inneholder en gyldig kolonne."

#. ucGyR
#: conn_shared_res.src
msgctxt "STR_INVALID_PARA_COUNT"
msgid "The count of the given parameter values doesn't match the parameters."
msgstr ""
"Tallet i den valgte parameterverdien stemmer ikke overens med parametrene."

#. 3EDJB
#: conn_shared_res.src
msgctxt "STR_NO_VALID_FILE_URL"
msgid "The URL '$URL$' is not valid. A connection can not be created."
msgstr "Lenka «$URL$» er ugyldig og ingen forbindelse kan opprettes."

#. 9n4j2
#: conn_shared_res.src
msgctxt "STR_NO_CLASSNAME"
msgid "The driver class '$classname$' could not be loaded."
msgstr "Klarte ikke å laste inn driverklassen «$classname$»."

#. jbnZZ
#: conn_shared_res.src
msgctxt "STR_NO_JAVA"
msgid "No Java installation could be found. Please check your installation."
msgstr "Fant ingen javainstallasjon. Sjekk installasjonen."

#. iKnFy
#: conn_shared_res.src
msgctxt "STR_NO_RESULTSET"
msgid "The execution of the query doesn't return a valid result set."
msgstr "Utførelsen av spørringa gir ikke et gyldig resultatsett."

#. kiYDS
#: conn_shared_res.src
msgctxt "STR_NO_ROWCOUNT"
msgid "The execution of the update statement doesn't effect any rows."
msgstr "Utførelsen av oppdateringsangivelsen påvirker ingen rader."

#. xiRq3
#: conn_shared_res.src
msgctxt "STR_NO_CLASSNAME_PATH"
msgid "The additional driver class path is '$classpath$'."
msgstr "Stien til den ekstra driverklassen er «$classpath$»."

#. QxNVP
#: conn_shared_res.src
msgctxt "STR_UNKNOWN_PARA_TYPE"
msgid "The type of parameter at position '$position$' is unknown."
msgstr "Parametertypen på plasseringa '$position$' er ukjent."

#. ghuVV
#: conn_shared_res.src
msgctxt "STR_UNKNOWN_COLUMN_TYPE"
msgid "The type of column at position '$position$' is unknown."
msgstr "Kolonnetypen på plasseringa '$position$' er ukjent."

#. 2M8qG
#: conn_shared_res.src
msgctxt "STR_NO_KDE_INST"
msgid "No suitable KDE installation was found."
msgstr "Fant ingen passende KDE-installasjon."

#. aCWED
#: conn_shared_res.src
msgctxt "STR_KDE_VERSION_TOO_OLD"
msgid ""
"KDE version $major$.$minor$ or higher is required to access the KDE Address "
"Book."
msgstr ""
"Trenger KDE-versjonen «$major$.$minor$» eller høyere for å kontakte KDEs "
"adressebok."

#. LVhyq
#: conn_shared_res.src
msgctxt "STR_KDE_VERSION_TOO_NEW"
msgid ""
"The found KDE version is too new. Only KDE up to version $major$.$minor$ is "
"known to work with this product.\n"
msgstr ""
"Den KDE-versjonen som ble funnet er for ny. Dette produktet er bare kent å "
"virke opp til versjon «$major$.$minor$» av KDE.\n"

#. gnHHh
#: conn_shared_res.src
msgctxt "STR_KDE_VERSION_TOO_NEW_WORK_AROUND"
msgid ""
"If you are sure that your KDE version works, you might execute the following Basic macro to disable this version check:\n"
"\n"
msgstr ""
"Hvis du er sikker på at din KDE-versjon virker, så kan du utføre følgende Basic-makro for å skru av denne versjonstesten:\n"
"\n"

#. 3FmFX
#: conn_shared_res.src
msgctxt "STR_PARA_ONLY_PREPARED"
msgid "Parameters can appear only in prepared statements."
msgstr "Parametre kan bare legges inn i behandlede uttrykk."

#. CB7pj
#: conn_shared_res.src
msgctxt "STR_NO_TABLE"
msgid "No such table!"
msgstr "Finnes ingen slik tabell!"

#. CDY8S
#: conn_shared_res.src
msgctxt "STR_NO_MAC_OS_FOUND"
msgid "No suitable Mac OS installation was found."
msgstr "Fant ingen passende Mas OS-installasjon."

#. HNSzq
#: conn_shared_res.src
msgctxt "STR_NO_STORAGE"
msgid "The connection can not be established. No storage or URL was given."
msgstr "Kan ikke opprette forbindelsen. Ingen lagring eller URL ble angitt."

#. SZSmZ
#: conn_shared_res.src
msgctxt "STR_INVALID_FILE_URL"
msgid ""
"The given URL contains no valid local file system path. Please check the "
"location of your database file."
msgstr ""
"Den oppgitte lenka inneholder ingen gyldig lokal filsystemsti. Sjekk hvor "
"databasen er plassert."

#. muHcn
#: conn_shared_res.src
msgctxt "STR_NO_TABLE_CONTAINER"
msgid "An error occurred while obtaining the connection's table container."
msgstr ""
"Det oppsto en feil under hentinga av tabellbeholderen til forbindelsen."

#. Rm4Le
#: conn_shared_res.src
msgctxt "STR_NO_TABLENAME"
msgid "There is no table named '$tablename$'."
msgstr "Det finnes ingen tabell som heter «$tablename$»."

#. 3BxCF
#: conn_shared_res.src
msgctxt "STR_NO_DOCUMENTUI"
msgid "The provided DocumentUI is not allowed to be NULL."
msgstr "Den angitte «DocumentUI» kan ikke være «NULL»."

#. VLEMM
#: conn_shared_res.src
msgctxt "STR_ERROR_NEW_VERSION"
msgid ""
"The connection could not be established. The database was created by a newer"
" version of %PRODUCTNAME."
msgstr ""
"Forbindelsen kunne ikke etableres. Databasen ble opprettet med en nyere "
"versjon av %PRODUCTNAME."
