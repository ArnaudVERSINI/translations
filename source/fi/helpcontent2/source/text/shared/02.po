"par_id0122200902231573\n"
"help.text"
msgid "<ahelp hid=\".\" visibility=\"hidden\">Opens the Hyperlink dialog.</ahelp>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">Avataan hyperlinkki-valintaikkuna.</ahelp>"

#: 09070000.xhp
