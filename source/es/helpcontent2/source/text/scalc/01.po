msgctxt ""
"04060184.xhp\n"
"par_id3153573\n"
"122\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_QUANTILSRANG\">Returns the percentage rank of a value in a sample.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_QUANTILSRANG\">Calcula el rango porcentual de un valor en una muestra.</ahelp>"

