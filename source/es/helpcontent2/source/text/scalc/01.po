msgctxt ""
"04060184.xhp\n"
"par_id3147401\n"
"154\n"
"help.text"
msgid "<item type=\"input\">=MINA(1;\"Text\";20)</item> returns 0."
msgstr "<item type=\"input\">=MÍNA(1;\"Texto\";20)</item> devuelve 0."

#: 04060184.xhp
msgctxt ""
"04060184.xhp\n"
"par_id3147295\n"
"155\n"
"help.text"
