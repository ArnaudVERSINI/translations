msgctxt ""
"references.xhp\n"
"par_id3151092\n"
"55\n"
"help.text"
msgid "Click <emph>Close</emph> when finished."
msgstr "完成选择后，单击<emph>关闭</emph>。"

#: references.xhp
msgctxt ""
"references.xhp\n"
