"hd_id3153326\n"
"159\n"
"help.text"
msgid "Syntax"
msgstr "Sintaxe"

#: 04060184.xhp
msgctxt ""
"04060184.xhp\n"
"par_id3149734\n"
"160\n"
"help.text"
msgid "AVERAGEA(Value1; Value2; ... Value30)"
