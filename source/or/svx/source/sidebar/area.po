msgctxt ""
"AreaPropertyPanel.src\n"
"RID_SIDEBAR_AREA_PANEL.LB_TRGR_TYPES\n"
"2\n"
"stringlist.text"
msgid "Solid"
msgstr "କଠିନ"

#: AreaPropertyPanel.src
msgctxt ""
"AreaPropertyPanel.src\n"
"RID_SIDEBAR_AREA_PANEL.LB_TRGR_TYPES\n"
"3\n"
"stringlist.text"
msgid "Linear"
msgstr "ସରଳ"

#: AreaPropertyPanel.src
msgctxt ""
"AreaPropertyPanel.src\n"
