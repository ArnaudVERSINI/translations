msgid "<link href=\"text/swriter/01/05060200.xhp\" name=\"No Wrap\">No Wrap</link>"
msgstr "<link href=\"text/swriter/01/05060200.xhp\" name=\"Kein Umlauf\">Kein Umlauf</link>"

#: main0216.xhp
msgctxt ""
