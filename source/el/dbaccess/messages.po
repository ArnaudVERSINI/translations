#: macromigration.src
msgctxt "STR_FORM"
msgid "Form '$name$'"
msgstr "Φόρμα '$name$'"

#. DBmvF
#: macromigration.src
msgctxt "STR_REPORT"
msgid "Report '$name$'"
msgstr "Έκθεση '$name$'"

#. CoMty
#: macromigration.src
msgctxt "STR_OVERALL_PROGRESS"
msgid "document $current$ of $overall$"
msgstr "έγγραφο $current$ από $overall$"

#. Ttf95
#: macromigration.src
msgctxt "STR_DATABASE_DOCUMENT"
msgid "Database Document"
msgstr "Έγγραφο βάσης δεδομένων"

#. 87ADQ
#: macromigration.src
msgctxt "STR_SAVED_COPY_TO"
msgid "saved copy to $location$"
msgstr "αποθηκεύτηκε αντίγραφο στο $location$"

#. BrPwb
#: macromigration.src
msgctxt "STR_MOVED_LIBRARY"
msgid "migrated $type$ library '$old$' to '$new$'"
msgstr "μεταφέρθηκε η $type$ βιβλιοθήκη '$old$' σε '$new$'"

#. C7CAR
#: macromigration.src
msgctxt "STR_LIBRARY_TYPE_AND_NAME"
msgid "$type$ library '$library$'"
msgstr "$type$ βιβλιοθήκη '$library$'"

#. E4RNh
#: macromigration.src
msgctxt "STR_MIGRATING_LIBS"
msgid "migrating libraries ..."
msgstr "μετακόμιση βιβλιοθηκών ..."

#. gDCmu
#: macromigration.src
msgctxt "STR_OOO_BASIC"
msgid "%PRODUCTNAME Basic"
msgstr "%PRODUCTNAME Basic"

#. pTV6D
#: macromigration.src
msgctxt "STR_JAVA_SCRIPT"
msgid "JavaScript"
msgstr "JavaScript"

#. X2qBp
#: macromigration.src
msgctxt "STR_BEAN_SHELL"
msgid "BeanShell"
msgstr "BeanShell"

#. FDBSE
#: macromigration.src
msgctxt "STR_JAVA"
msgid "Java"
msgstr "Java"

#. F4yo3
#: macromigration.src
msgctxt "STR_PYTHON"
msgid "Python"
msgstr "Python"

#. BwGMo
#: macromigration.src
msgctxt "STR_DIALOG"
msgid "dialog"
msgstr "διάλογος"

#. VXkqp
#: macromigration.src
msgctxt "STR_ERRORS"
msgid "Error(s)"
msgstr "Σφάλμα(τα)"

#. BVTZE
#: macromigration.src
msgctxt "STR_WARNINGS"
msgid "Warnings"
msgstr "Προειδοποιήσεις"

#. wArT8
#: macromigration.src
msgctxt "STR_EXCEPTION"
msgid "caught exception:"
msgstr "λήψη εξαίρεσης:"

#. 3fagJ
#: macromigration.src
msgctxt "STR_INVALID_BACKUP_LOCATION"
msgid ""
"You need to choose a backup location other than the document location "
"itself."
msgstr ""
"Πρέπει να επιλέξετε μια τοποθεσία για αντιγραφή διαφορετική από τη θέση του "
"ίδιου του εγγράφου."

#. Zj8Ti
#: macromigration.src
msgctxt "STR_INVALID_NUMBER_ARGS"
msgid "Invalid number of initialization arguments. Expected 1."
msgstr "Άκυρος αριθμός παραμέτρων αρχικοποίησης. Αναμένεται 1."

#. N6oEB
#: macromigration.src
msgctxt "STR_NO_DATABASE"
msgid "No database document found in the initialization arguments."
msgstr "Δεν βρέθηκε έγγραφο βάσης δεδομένων στις παραμέτρους αρχικοποίησης."

#. SvACu
#: macromigration.src
msgctxt "STR_NOT_READONLY"
msgid "Not applicable to read-only documents."
msgstr "Δεν εφαρμόζεται στα μόνο για ανάγνωση έγγραφα."

#. KyLuN
#: copytablewizard.src
msgctxt "STR_CTW_NO_VIEWS_SUPPORT"
msgid "The destination database does not support views."
msgstr "Η βάση δεδομένων προορισμού δεν υποστηρίζει προβολές."

#. RaJQd
#: copytablewizard.src
msgctxt "STR_CTW_NO_PRIMARY_KEY_SUPPORT"
msgid "The destination database does not support primary keys."
msgstr "Η βάση δεδομένων προορισμού δεν υποστηρίζει πρωτεύοντα κλειδιά."

#. JBBmY
#: copytablewizard.src
