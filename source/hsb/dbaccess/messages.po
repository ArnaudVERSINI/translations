msgid "$type$ library '$library$'"
msgstr "biblioteka $type$ '$library$'"

#. E4RNh
#: macromigration.src
msgctxt "STR_MIGRATING_LIBS"
msgid "migrating libraries ..."
msgstr "biblioteki so migruja ..."

