msgctxt ""
"07060000.xhp\n"
"par_id3151315\n"
"2\n"
"help.text"
msgid "<ahelp hid=\".uno:Reload\" visibility=\"visible\">Replaces the current document with the last saved version.</ahelp>"
msgstr "<ahelp hid=\".uno:Reload\" visibility=\"visible\">Galmee ammee maxxansa xumura olkaa'ameen bakka buusi.</ahelp>"

#: 07060000.xhp
msgctxt ""
