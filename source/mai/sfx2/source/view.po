msgstr "<HTML><BODY BGCOLOR=\"#c0c0c0\"><BR><BR><P><FONT FACE=\"Arial\"><B>कोनो दस्तावेज विशेषतासभ नहि भेटल ।</B></FONT></P></BODY></HTML>"

