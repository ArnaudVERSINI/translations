msgctxt ""
"01010200.xhp\n"
"par_id3149413\n"
"53\n"
"help.text"
msgid "Examples"
msgstr "<emph>Esempi</emph>"

#: 01010200.xhp
msgctxt ""
"01010200.xhp\n"
"par_id3148455\n"
"54\n"
"help.text"
msgid "File system"
msgstr "<emph>File system</emph>"

#: 01010200.xhp
msgctxt ""
"01010200.xhp\n"
"par_id3150715\n"
"55\n"
"help.text"
msgid "Internet"
msgstr "<emph>Internet</emph>"

