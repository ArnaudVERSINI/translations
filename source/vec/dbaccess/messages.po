#: macromigration.src
msgctxt "STR_STATE_CLOSE_SUB_DOCS"
msgid "Prepare"
msgstr "Pareciar"

#. pFNR3
#: macromigration.src
msgctxt "STR_STATE_BACKUP_DBDOC"
msgid "Backup Document"
msgstr "Backup (copia de seguresa) de'l documento"

