"01170101.xhp\n"
"hd_id3153876\n"
"139\n"
"help.text"
msgid "Border"
msgstr "Marcu"

#: 01170101.xhp
msgctxt ""
"01170101.xhp\n"
"par_id3154017\n"
"140\n"
"help.text"
msgid "<ahelp hid=\".\">Specify the border type for the current control.</ahelp>"
msgstr "<ahelp hid=\".\">Especifique'l tipu de borde pal control.</ahelp>"

#: 01170101.xhp
