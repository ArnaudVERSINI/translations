"help.text"
msgid "<ahelp hid=\"OFFMGR:PUSHBUTTON:RID_OFAPAGE_AUTOCORR_EXCEPT:PB_NEWDOUBLECAPS\">Adds the current entry to the list of exceptions.</ahelp>"
msgstr "<ahelp hid=\"OFFMGR:PUSHBUTTON:RID_OFAPAGE_AUTOCORR_EXCEPT:PB_NEWDOUBLECAPS\">Lägger till den aktuella posten i listan med undantag.</ahelp>"

#. }*^w
