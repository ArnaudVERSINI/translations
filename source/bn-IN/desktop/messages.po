#. s6iho
#: dp_gui_dialog.src
msgctxt "RID_CTX_ITEM_ENABLE"
msgid "~Enable"
msgstr "সক্রিয় (~E)"

#. CeKUw
#: dp_gui_dialog.src
msgctxt "RID_CTX_ITEM_DISABLE"
msgid "~Disable"
