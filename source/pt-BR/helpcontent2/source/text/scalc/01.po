msgctxt ""
"12090102.xhp\n"
"par_id3145647\n"
"23\n"
"help.text"
msgid "<ahelp hid=\"SC:MOREBUTTON:RID_SCDLG_PIVOT_LAYOUT:BTN_MORE\">Displays or hides additional options for defining the pivot table.</ahelp>"
msgstr "<ahelp hid=\"SC:MOREBUTTON:RID_SCDLG_PIVOT_LAYOUT:BTN_MORE\">Exibe ou oculta opções adicionais para definir a tabela dinâmica.</ahelp>"

