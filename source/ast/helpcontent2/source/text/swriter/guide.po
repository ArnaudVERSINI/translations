msgstr "<bookmark_value>formatu;sangrar párrafos</bookmark_value>      <bookmark_value>sangríes;en testu</bookmark_value>      <bookmark_value>párrafos;sangríes</bookmark_value>      <bookmark_value>colgar sangríes en párrafos</bookmark_value>      <bookmark_value>sangríes a la derecha en párrafos</bookmark_value>      <bookmark_value>llinies de testu;sangríes</bookmark_value>      <bookmark_value>camudar;sangríes</bookmark_value>"

#: indenting.xhp
msgctxt ""
