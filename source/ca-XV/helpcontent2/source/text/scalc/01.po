"102\n"
"help.text"
msgid "POISSON"
msgstr "POISSON"

#: 04060184.xhp
msgctxt ""
"04060184.xhp\n"
"par_id3154298\n"
"103\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_POISSON\">Returns the Poisson distribution.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_POISSON\">Retorna la distribució de Poisson.</ahelp>"

#: 04060184.xhp
