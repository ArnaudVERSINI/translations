msgctxt ""
"01170101.xhp\n"
"par_id3145251\n"
"4\n"
"help.text"
msgid "<ahelp hid=\".\">Specify the background color for the current control.</ahelp>"
msgstr "<ahelp hid=\".\">Especifique'l color de fondu del control.</ahelp>"

#: 01170101.xhp
