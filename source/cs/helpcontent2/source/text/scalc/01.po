msgctxt ""
"05100000.xhp\n"
"par_id3147299\n"
"25\n"
"help.text"
msgid "<ahelp hid=\"HID_TEMPLATE_FILTER\">Lists the available style groups.</ahelp>"
msgstr "<ahelp hid=\"HID_TEMPLATE_FILTER\">Vypíše dostupné skupiny stylů.</ahelp>"

#: 04070300.xhp
