"01060100.xhp\n"
"par_id3150440\n"
"6\n"
"help.text"
msgid "<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_FORMULA\">Specifies whether to show formulas instead of results in the cells.</ahelp>"
msgstr "<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_FORMULA\">Hücrenin değeri yerine formülün gösterilip gösterilmeyeceğini belirtir.</ahelp>"

#: 01060100.xhp
