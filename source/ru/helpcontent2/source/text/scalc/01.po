msgctxt ""
"04060182.xhp\n"
"hd_id3166466\n"
"30\n"
"help.text"
msgid "Syntax"
msgstr "Синтаксис"

#: 04060182.xhp
msgctxt ""
"04060182.xhp\n"
"par_id3153024\n"
"31\n"
"help.text"
msgid "FTEST(Data1; Data2)"
msgstr "FTEST(Данные 1; Данные 2)"

#: 04060182.xhp
msgctxt ""
"04060182.xhp\n"
"par_id3150032\n"
"32\n"
"help.text"
msgid "<emph>Data1</emph> is the first record array."
msgstr "<emph>Данные1</emph>: первый массив записей."

#: 04060182.xhp
msgctxt ""
"04060182.xhp\n"
"par_id3153018\n"
"33\n"
"help.text"
msgid "<emph>Data2</emph> is the second record array."
msgstr "<emph>Данные2</emph>: второй массив записей."

