#. raZKu
#: conn_shared_res.src
msgctxt "STR_TIMEOUT_WAITING"
msgid "Timed out while waiting for the result."
msgstr "Isteklo je vreme tokom čekanja rezultata."

#. 9LXDp
#: conn_shared_res.src
msgctxt "STR_ERR_EXECUTING_QUERY"
msgid "An error occurred while executing the query."
