msgstr "Zostava '$name$'"

#. CoMty
#: macromigration.src
msgctxt "STR_OVERALL_PROGRESS"
msgid "document $current$ of $overall$"
msgstr "dokument $current$ z $overall$"

#. Ttf95
#: macromigration.src
