msgctxt "SC_OPCODE_OFFSET"
msgid "Columns"
msgstr "Stolpci"

#: scfuncs.hrc:3436
msgctxt "SC_OPCODE_OFFSET"
msgid "The number of columns that are to be moved to the left or to the right."
msgstr "Število stolpcev, ki bodo premaknjeni levo ali desno."

#: scfuncs.hrc:3437
msgctxt "SC_OPCODE_OFFSET"
msgid "Height"
msgstr "Višina"

