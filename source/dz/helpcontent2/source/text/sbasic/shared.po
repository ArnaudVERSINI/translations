msgctxt ""
"03020405.xhp\n"
"par_id3155066\n"
"17\n"
"help.text"
msgid "4 - RANDOM (file open for random access)"
msgstr "༤ - གང་བྱུང་ (file open for random access)"

#: 03020405.xhp
msgctxt ""
"03020405.xhp\n"
"par_id3148406\n"
