"help.text"
msgid "<image src=\"res/sc10714.png\" id=\"img_id3153894\"><alt id=\"alt_id3153894\">Icon</alt></image>"
msgstr "<image src=\"res/sc10714.png\" id=\"img_id3153894\"><alt id=\"alt_id3153894\">Biểu tượng</alt></image>"

#: 12100000.xhp
