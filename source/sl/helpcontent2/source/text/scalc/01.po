msgctxt ""
"04060183.xhp\n"
"par_id3154372\n"
"5\n"
"help.text"
msgid "LARGE(Data; RankC)"
msgstr "LARGE(podatki; vrsta_c)"

#: 04060183.xhp
msgctxt ""
"04060183.xhp\n"
"par_id3152986\n"
"6\n"
"help.text"
msgid "<emph>Data</emph> is the cell range of data."
msgstr "<emph>Podatki</emph> je obseg celic podatkov."

#: 04060183.xhp
msgctxt ""
"04060183.xhp\n"
"par_id3156448\n"
"7\n"
"help.text"
msgid "<emph>RankC</emph> is the ranking of the value."
msgstr "<emph>Vrsta_c</emph> je razvrstitev vrednosti."

#: 04060183.xhp
msgctxt ""
"04060183.xhp\n"
"hd_id3152889\n"
"8\n"
"help.text"
msgid "Example"
msgstr "Primer"

#: 04060183.xhp
msgctxt ""
"04060183.xhp\n"
"par_id3148702\n"
"9\n"
"help.text"
msgid "<item type=\"input\">=LARGE(A1:C50;2)</item> gives the second largest value in A1:C50."
msgstr "<item type=\"input\">=LARGE(A1:C50;2)</item> vrne drugo največjo vrednost v A1:C50."

