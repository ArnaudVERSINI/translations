"help.text"
msgid "<item type=\"input\">=DGET(A1:E10;\"Name\";A13:E14)</item>"
msgstr "<item type=\"input\">=DGET(A1:E10;\"Maqaa\";A13:E14)</item>"

#. 8jg}
