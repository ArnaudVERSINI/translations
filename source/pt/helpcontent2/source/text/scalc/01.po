msgstr "Exemplo"

#: 04060184.xhp
msgctxt ""
"04060184.xhp\n"
"par_id3153122\n"
"33\n"
"help.text"
msgid "<item type=\"input\">=AVEDEV(A1:A50)</item>"
msgstr "<item type=\"input\">=DESV.MÉDIO(A1:A50)</item>"

#: 04060184.xhp
msgctxt ""
"04060184.xhp\n"
"bm_id3145824\n"
"help.text"
msgid "<bookmark_value>AVERAGE function</bookmark_value>"
msgstr "<bookmark_value>MÉDIA (função)</bookmark_value>"

#: 04060184.xhp
