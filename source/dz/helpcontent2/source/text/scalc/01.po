msgctxt ""
"04060116.xhp\n"
"par_id2927335\n"
"help.text"
msgid "For odd numbers FACTDOUBLE(n) returns:"
msgstr "ཡ་ཅན་ཨང་གྲངས་ཀྱི་དོན་ལུ་ FACTDOUBLE(n) སླར་ལོག་འབདཝ་ཨིན:"

#: 04060116.xhp
msgctxt ""
"04060116.xhp\n"
"par_id2107303\n"
"help.text"
msgid "1*3*5*7* ... *n"
msgstr "1*3*5*7* ... *n"

