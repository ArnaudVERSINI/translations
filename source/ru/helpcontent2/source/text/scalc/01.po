msgctxt ""
"04060183.xhp\n"
"par_id3155623\n"
"74\n"
"help.text"
msgid "<item type=\"input\">=LOGINV(0.05;0;1)</item> returns 0.19."
msgstr "<item type=\"input\">=LOGINV(0,05; 0;1)</item> возвращает 0,19."

