msgctxt ""
"01060500.xhp\n"
"par_id3147317\n"
"15\n"
"help.text"
msgid "Net"
msgstr "Ağ"

#: 01060500.xhp
msgctxt ""
"01060500.xhp\n"
"par_id3147348\n"
"16\n"
"help.text"
msgid "=B5-B7"
msgstr "=B5-B7"

#: 01060500.xhp
msgctxt ""
"01060500.xhp\n"
"par_id3154918\n"
"17\n"
"help.text"
msgid "7"
msgstr "7"

