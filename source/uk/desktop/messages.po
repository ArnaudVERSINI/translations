#. m6EtT
#: dp_gui_updatedialog.src
msgctxt "RID_DLG_UPDATE_IGNORE"
msgid "Ignore this Update"
msgstr "Ігнорувати це оновлення"

#. thoRv
#: dp_gui_updatedialog.src
msgctxt "RID_DLG_UPDATE_IGNORE_ALL"
msgid "Ignore all Updates"
msgstr "Пропустити всі оновлення"

#. GBGe5
