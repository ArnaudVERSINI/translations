"par_id3151393\n"
"65\n"
"help.text"
msgid "<item type=\"input\">=IMPOWER(\"2+3i\";2)</item> returns -5+12i."
msgstr "<item type=\"input\">=IMPOT(\"2+3i\";2)</item> retorna -5+12i."

