"text_nav_keyb.xhp\n"
"par_id3155944\n"
"50\n"
"help.text"
msgid "PgUp"
msgstr "PgUp"

#: text_nav_keyb.xhp
msgctxt ""
"text_nav_keyb.xhp\n"
"par_id3148678\n"
"54\n"
"help.text"
msgid "Scrolls up one page."
msgstr "Fuula tokko ol mara."

