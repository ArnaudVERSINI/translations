"10\n"
"help.text"
msgid "<ahelp hid=\"HID_OPTIONS_COLORCONFIG_NAME_SCHEME\">Enter a name for the color scheme.</ahelp>"
msgstr "<ahelp hid=\"HID_OPTIONS_COLORCONFIG_NAME_SCHEME\">Indtast et navn for farveskemaet.</ahelp>"

