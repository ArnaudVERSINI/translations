"04050000.xhp\n"
"par_id3153714\n"
"10\n"
"help.text"
msgid "<ahelp hid=\"SC_RADIOBUTTON_RID_SCDLG_INSERT_TABLE_RB_FROMFILE\">Inserts a sheet from an existing file into the current document.</ahelp>"
msgstr "<ahelp hid=\"SC_RADIOBUTTON_RID_SCDLG_INSERT_TABLE_RB_FROMFILE\">Insere uma planilha no documento atual a partir de um arquivo existente.</ahelp>"

#: 04050000.xhp
msgctxt ""
"04050000.xhp\n"
