"04060103.xhp\n"
"par_id3147246\n"
"401\n"
"help.text"
msgid "<item type=\"input\">=RECEIVED(\"1999-02-15\";\"1999-05-15\";1000;0.0575;2)</item> returns 1014.420266."
msgstr "<item type=\"input\">=RECEIVED(\"15.2.1999\";\"15.5.1999\";1000;0,0575;2)</item> vrne 1014,420266."

#: 04060103.xhp
