msgstr "<bookmark_value>функция BIN2DEC</bookmark_value><bookmark_value>преобразование; двоичные числа, в десятичные числа</bookmark_value>"

#: 04060115.xhp
msgctxt ""
"04060115.xhp\n"
"hd_id3153034\n"
"17\n"
"help.text"
msgid "BIN2DEC"
msgstr "BIN2DEC"

#: 04060115.xhp
msgctxt ""
"04060115.xhp\n"
"par_id3144744\n"
"18\n"
"help.text"
msgid "<ahelp hid=\"HID_AAI_FUNC_BIN2DEC\">The result is the decimal number for the binary number entered.</ahelp>"
msgstr "<ahelp hid=\"HID_AAI_FUNC_BIN2DEC\">Преобразует указанное двоичное число в десятичное.</ahelp>"

#: 04060115.xhp
msgctxt ""
"04060115.xhp\n"
"hd_id3145593\n"
"19\n"
"help.text"
msgid "Syntax"
msgstr "Синтаксис"

