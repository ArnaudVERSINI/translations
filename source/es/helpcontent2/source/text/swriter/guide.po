"help.text"
msgid "Key"
msgstr "Tecla"

#: text_nav_keyb.xhp
msgctxt ""
"text_nav_keyb.xhp\n"
"par_id3155870\n"
"24\n"
"help.text"
msgid "Function"
msgstr "Función"

#: text_nav_keyb.xhp
msgctxt ""
"text_nav_keyb.xhp\n"
"par_id3156220\n"
"25\n"
"help.text"
msgid "<emph>+</emph><switchinline select=\"sys\"><caseinline select=\"MAC\"><emph>Command key</emph></caseinline><defaultinline><emph>Ctrl key</emph></defaultinline></switchinline>"
