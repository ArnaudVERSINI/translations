msgstr "IM.POT(\"Númberu Complexu\"; Númberu)"

#: 04060116.xhp
msgctxt ""
"04060116.xhp\n"
"par_id3155743\n"
"63\n"
"help.text"
msgid "<emph>Number</emph> is the exponent."
msgstr "<emph>Númberu</emph> ye l'esponente."

#: 04060116.xhp
msgctxt ""
"04060116.xhp\n"
"hd_id3149048\n"
