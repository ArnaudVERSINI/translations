"help.text"
msgid "<ahelp hid=\"svx:PushButton:RID_SVXDLG_JAVA_PARAMETER:PB_REMOVE\">Deletes the selected JRE start parameter.</ahelp>"
msgstr "<ahelp hid=\"svx:PushButton:RID_SVXDLG_JAVA_PARAMETER:PB_REMOVE\">Tar bort den markerade JRE-startparametern.</ahelp>"

