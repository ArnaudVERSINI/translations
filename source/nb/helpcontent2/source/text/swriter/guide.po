msgctxt ""
"text_capital.xhp\n"
"par_id3147420\n"
"9\n"
"help.text"
msgid "Select the text that you want to capitalize."
msgstr "Velg teksten du vil endre på."

#: text_capital.xhp
msgctxt ""
"text_capital.xhp\n"
"par_id3149841\n"
"10\n"
"help.text"
msgid "Do one of the following:"
msgstr "Gjør dette:"

