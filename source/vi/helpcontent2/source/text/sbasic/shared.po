msgctxt ""
"01170101.xhp\n"
"par_idN10EEB\n"
"help.text"
msgid "Click the <emph>...</emph> button to open the <emph>Selection</emph> dialog."
msgstr "Nhấn vào nút <emph>...</emph> để mở hộp thoại <emph>Lựa chọn</emph>."

