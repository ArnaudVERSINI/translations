msgid "<image id=\"img_id3153311\" src=\"cmd/sc_sbaexecutesql.png\" width=\"0.222inch\" height=\"0.222inch\"><alt id=\"alt_id3153311\">Icon</alt></image>"
msgstr "<image src=\"cmd/sc_sbaexecutesql.png\" id=\"img_id3153311\"><alt id=\"alt_id3153311\">Ícone</alt></image>"

#: 14010000.xhp
msgctxt ""
"14010000.xhp\n"
"par_id3153684\n"
"4\n"
"help.text"
msgid "Run Query"
msgstr "Executar consulta"

