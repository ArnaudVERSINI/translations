msgctxt "STR_QUERY_UNDO_INSERTCONNECTION"
msgid "Insert Join"
msgstr "Inserisci collegamento"

#. isNxK
#: query.src
msgctxt "STR_QUERY_UNDO_REMOVECONNECTION"
msgid "Delete Join"
msgstr "Termina collegamento"

#. 9Uu5p
