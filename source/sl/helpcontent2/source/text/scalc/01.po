msgctxt ""
"04060184.xhp\n"
"par_id3155589\n"
"135\n"
"help.text"
msgid "<emph>Type</emph> represents the type of quartile. (0 = MIN, 1 = 25%, 2 = 50% (MEDIAN), 3 = 75% and 4 = MAX.)"
msgstr "<emph>Vrsta</emph> predstavlja vrsto četrtinske vrednosti. (0 = MIN, 1 = 25%, 2 = 50% (MEDIAN), 3 = 75% in 4 = MAX.)"

#: 04060184.xhp
msgctxt ""
"04060184.xhp\n"
"hd_id3149103\n"
"136\n"
"help.text"
msgid "Example"
msgstr "Primer"

