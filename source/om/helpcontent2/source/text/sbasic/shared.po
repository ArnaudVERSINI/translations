msgctxt ""
"01170101.xhp\n"
"hd_id3152778\n"
"131\n"
"help.text"
msgid "Date min."
msgstr "Guyyaa jalantaa"

#: 01170101.xhp
msgctxt ""
"01170101.xhp\n"
"par_id3154120\n"
"132\n"
"help.text"
msgid "<ahelp hid=\".\">Specify the lower limit for a date control.</ahelp>"
msgstr "<ahelp hid=\".\">Too`annaa guyyaaf daangaa jalaa ifteessi.</ahelp>"

