#: comment.xhp
msgctxt ""
"comment.xhp\n"
"par_idN105D3\n"
"help.text"
msgid "a^2+b^2=c^2 %% Pythagorean theorem."
msgstr "a^2+b^2=c^2 %% Der Satz des Pythagoras"

