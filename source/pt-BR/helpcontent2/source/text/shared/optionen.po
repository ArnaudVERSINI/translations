"par_id3159413\n"
"10\n"
"help.text"
msgid "<ahelp hid=\"HID_OPTIONS_COLORCONFIG_NAME_SCHEME\">Enter a name for the color scheme.</ahelp>"
msgstr "<ahelp hid=\"HID_OPTIONS_COLORCONFIG_NAME_SCHEME\">Insira um nome para o esquema de cores.</ahelp>"

#: 01012000.xhp
