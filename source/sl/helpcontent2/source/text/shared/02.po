"par_id3147209\n"
"8\n"
"help.text"
msgid "<ahelp hid=\"HID_ICCDIALOG_OK_BTN\">Applies the data to your document.</ahelp>"
msgstr "<ahelp hid=\"HID_ICCDIALOG_OK_BTN\">Doda podatke vašemu dokumentu.</ahelp>"

#: 09070000.xhp
