"bm_id3150372\n"
"help.text"
msgid "<bookmark_value>AREAS function</bookmark_value>"
msgstr "<bookmark_value>TERÜLET függvény</bookmark_value><bookmark_value>AREAS függvény, lásd: TERÜLET függvény</bookmark_value>"

#. bH,3
