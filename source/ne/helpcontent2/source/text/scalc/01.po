"04060182.xhp\n"
"par_id3145073\n"
"10\n"
"help.text"
msgid "<item type=\"input\">=FINV(0.5;5;10)</item> yields 0.93."
msgstr "=FINV(0.5; 5; 10) उपजहरू ०.९३ ।"

