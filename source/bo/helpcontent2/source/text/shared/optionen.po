msgctxt ""
"01070500.xhp\n"
"hd_id3156383\n"
"24\n"
"help.text"
msgid "Start presentation (only in presentations)"
msgstr "གསལ་སྟོན་ཡིག་ཟིན་སྒུལ་བ་(གསལ་སྟོན་ཡིག་ཟིན་ཁོ་ནར་སྤྱོད་པ་)"

