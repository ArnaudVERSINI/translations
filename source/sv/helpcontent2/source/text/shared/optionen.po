msgctxt ""
"01070500.xhp\n"
"par_id3149578\n"
"2\n"
"help.text"
msgid "<ahelp hid=\"HID_SD_OPTIONS_MISC\">Defines the general options for drawing or presentation documents.</ahelp>"
msgstr "<ahelp hid=\"HID_SD_OPTIONS_MISC\">Definierar allmänna alternativ för tecknings- och presentationsdokument.</ahelp>"

#: 01070500.xhp
msgctxt ""
"01070500.xhp\n"
