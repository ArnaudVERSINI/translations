msgctxt ""
"02010100.xhp\n"
"par_id3148478\n"
"196\n"
"help.text"
msgid "SELECT \"PtNo\" AS \"PartNum\" FROM \"Parts\""
msgstr "SELECT \"NrCz\" AS \"NumerCzesci\" FROM \"Czesci\""

